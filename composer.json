{
    "name": "mradang/laravel-options",
    "description": "laravel-options",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mradang",
            "email": "mradang@163.com"
        }
    ],
    "require": {
        "laravel/framework": "^9.0 || ^10.0"
